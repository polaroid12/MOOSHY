print("help mooshy find", 30,60)
print("its friends", 40,70)
print("press ❎ to start",30,80)
end

function draw_game()
  cls()
  map(0,0)
  spr(player.sp,player.x,player.y,1,1,player.flp)
		
		if (btn(⬇️)) show_inventory()

		draw_pickups()

end
