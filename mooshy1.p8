function update_menu()
		if btnp(❎) then
		
		init_game()
		end
	end

function update_game()
  player_update()
  player_animate()
  
  update_pickups()

  --simple camera
  cam_x=player.x-64+(player.w/2)
  if cam_x<map_start then
     cam_x=map_start
  end
  if cam_x>map_end-128 then
     cam_x=map_end-128
  end
  camera(cam_x,0)
end

